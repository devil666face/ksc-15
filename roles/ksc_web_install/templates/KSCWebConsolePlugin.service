[Unit]
Description=Kaspersky Security Center Web Console Plugin

[Service]
Type=simple
WorkingDirectory=/var/opt/kaspersky/ksc-web-console
User=user_web_plugin_tyuaw733eo
CapabilitiesParsec=PARSEC_CAP_PRIV_SOCK
ExecStart=/usr/bin/env /var/opt/kaspersky/ksc-web-console/node /var/opt/kaspersky/ksc-web-console/pm.plugin.js
TimeoutSec=1
Restart=always
RestartSec=1

[Install]
WantedBy=multi-user.target
